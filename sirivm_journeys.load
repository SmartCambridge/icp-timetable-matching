LOAD CSV
    FROM stdin WITH ENCODING utf8
    INTO postgresql:///icp?sirivm_journies
       TARGET COLUMNS

       (  day,
          departure_time,
          origin,
          destination,
          line_name,
          operator_code,
          vehicle_ref
       )
    WITH truncate,
         fields enclosed by '"',
         fields escaped by backslash-quote,
         fields terminated by ','

    SET work_mem to '32 MB', maintenance_work_mem to '64 MB';